algoritmo "equacao"
// Função :
// Autor :
// Data : 03/08/2023
// Seção de Declarações
var
a,b,c,d,x1,x2: inteiro
inicio
escreval("DIGITE A:")
leia(a)
escreval("DIGITE B: ")
leia(b)
escreval("DIGITE C: ")
leia(c)

d <- (b*b) - 4*a*c
x1 <- (-(b) + raizq(d))/2*a
x2 <- (-(b) - raizq(d))/2*a

escreval("x1, x2")
escreval(x1)
escreval(x2)

fimalgoritmo
